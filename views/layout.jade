!!!
html
  head
    title= title
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz&subset=latin', type='text/css')
    link(rel='stylesheet', href='/css/style.css')
    script(type='text/javascript', src='http://ajax.googleapis.com/ajax/libs/jquery/1.4.3/jquery.min.js')
    script(type='text/javascript', src='/Socket.IO/socket.io.js')
    script(type='text/javascript', src='/js/main.js')
  body
    #wrapper
      #header 
        #logo
          h1
            a(href="/", title="Thumb War Home") Thumb War
        ul#menu
          li 
            a#signup(href="#") Sign Up
          li 
            a#signin(href="#") Sign In
          li
            a#faq(href="/about") About
      #content!= body
        #signup-form
          .close x
          h2 Create an account to claim your score
          form
            input#phone(type='text', placeholder='Phone Number', name='phone')
            input#username(type='text', placeholder='Desired Username', name='username')
            input#password(type='password', placeholder='Password', name='password')
            input#submit(type='submit', value="Sign Up")
        #signin-form
          .close x
          h2 Sign In
          form
            input#username(type='text', placeholder='Username', name='username')
            input#password(type='password', placeholder='Password', name='password')
            input#submit(type='submit', value="Sign In")
